{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://fencestexas.com/:path*",
      "statusCode": 301
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" },
        { "key": "X-Robots-Tag", "value": "all" }
      ]
    }
  ]
}
